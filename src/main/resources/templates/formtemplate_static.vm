								#if($schemaJson)
					        	var schema = JSON.parse('$schemaJson');
					        	
					        	function generate(schema, heirar, isnm, extra, addclas, labinpdet)
								{
									  if(schema.type=='string' || schema.type=='number' || schema.type=='integer' 
									  		|| schema.type=='boolean' || schema.type=='date' || schema.type=='float'
									  		|| schema.type=='double')
									  {
									  		var valut = generateRandom(schema.type);
									  		var nmdef = (isnm?("name=\""+heirar+"\""):"");
											var dtcls = "";
											if(schema.type=='date')
												dtcls = "clsDatePicker";
											
											if(schema.hasOwnProperty('enum'))
											{
												var htm = "<div class=\"form-elems controls\"><label class=\""+addclas+"\" "+labinpdet+">"+heirar+"</label><br/><select "+labinpdet+" "+nmdef+" "+extra+" class=\"form-control "+addclas+""+dtcls+"\">";
												for(var i=0;i<schema.enum.length;i++)
													htm += "<option value=\""+schema.enum[i]+"\">"+schema.enum[i]+"</option>";
												return htm + ("</select><br/><br/></div>");
											}
											else
												return ("<div class=\"form-elems\"><label class=\""+addclas+"\" "+labinpdet+">"+heirar+"</label><input "+labinpdet+" "+nmdef+" "+extra+" class=\"form-control "+addclas+""+dtcls+"\" onblur=\"validate(this, '"+schema.type+"')\" value=\""+valut+"\" type='text'/><br/><br/></div>");
									  }
									  else if(schema.type=='object')
									  {
											if(schema.hasOwnProperty('properties'))
											{
												var html = "<div>";
												for (var property in schema.properties) {
													if (schema.properties.hasOwnProperty(property)) {
														var hirNm = heirar;
														if(!hirNm || hirNm=="")
															hirNm = property;
														else
															hirNm += "['"+property+"']";
														//var hirNm = (!heirar?"":heirar+"[") + property + "]";
														var propobj = schema.properties[property];
														//var margin = "style='margin-left:"+(step*5)+"px'";
														var htmlp = generate(propobj, hirNm, isnm, "", addclas, "orig_val_cls=\""+hirNm+"\"");
														html += htmlp;
													}
												}				
												return html + "</div>";
											}
									  }
									  else if(schema.type=='array')
									  {
											if(schema.hasOwnProperty('items'))
											{
												//var hirNm = (!heirar?"":heirar);
												var hirNm = heirar;	
												if(!hirNm || hirNm=="")
													hirNm = "";
												if(!countMap.hasOwnProperty(hirNm))
													countMap[hirNm] = 0;
												var ohirNm = hirNm;
												schemaMap[hirNm] = schema.items;
												
												hirNm += "["+(countMap[hirNm]++)+"]";
												//hirNm = (hirNm+"[" + (countMap[hirNm]++) +"]");
												//var eleid = hirNm.replace('.','').replace('[','').replace(']','');
												var html = '<div id="_element_'+ohirNm+'" class="form-elems" style="border:1px dotted black;padding:10px;margin-top:10px;">'+ohirNm+'<a href="#" style="text-decoration: none" onclick="addRemListElement(\''+ohirNm+'\',true)">&nbsp;&nbsp;<b>+</b></a><a href="#" style="text-decoration: none" onclick="addRemListElement(\''+ohirNm+'\',false)">&nbsp;&nbsp;<b>-</b></a><br/>';
												html += generate(schema.items, hirNm, isnm, "", addclas, "orig_val_cls=\""+hirNm+"\"");
												html += "</div>";				
												return html;
											}
									  }
									  else if(schema.type=='map')
									  {
											//var hirNm = (!heirar?"":heirar)
											var hirNm = heirar;
											if(!hirNm)
												hirNm = "";
											if(schema.hasOwnProperty('types'))
											{
												if(schema.types.hasOwnProperty('key') && schema.types.hasOwnProperty('value'))
												{
													var eleid = hirNm;//.replace('.','').replace('[','').replace(']','');
													var html = '<div id="_element_'+eleid+'" class="form-elems" style="border:1px dotted black;padding:10px;margin-top:10px;">'+hirNm+'<a href="#" onclick="addRemMapElement(\''+hirNm+'\', true)"><b>&nbsp;&nbsp;+</b></a><a href="#" style="text-decoration: none" onclick="addRemMapElement(\''+hirNm+'\', false)"><b>&nbsp;&nbsp;-</b></a><br/>';
													var evnt = "onblur=\"updateMapValueNms('"+hirNm+"',this)\" map_value_cls=\""+hirNm+"\"";
													html += generate(schema.types.key, hirNm, false, evnt, "map_key_cls", "");
													var cls = hirNm + " " + addclas;
													html += generate(schema.types.value, hirNm, true, "", cls, "orig_val_cls=\""+hirNm+"\"");
													html += "</div>";
													schemaMap[hirNm] = schema.types;
													return html;
												}
											}
									  }
								}
					
								function updateMapValueNms(clsnm, key)
								{
									if(key.value.trim()=="")return;
									if(!key.hasOwnProperty('lastclsnm'))
										key.lastclsnm = "";
									var nclsnm = clsnm;
									if(nclsnm.lastIndexOf("[")!=-1)
										nclsnm = nclsnm.substring(0, nclsnm.lastIndexOf("[")) + "['" + key.value + "']" + nclsnm.substring(nclsnm.lastIndexOf("["));
									else
										nclsnm += "['" + key.value + "']";
									$('.'+clsnm).each(function() {
										
										this.className = this.className.replace(key.lastclsnm, "");
										this.className += " " + nclsnm + " ";					
										if(this.nodeName.toLowerCase()=="input")
										{
											var nnnm = this.getAttribute('orig_val_cls');
											if(nnnm.indexOf("[")!=-1)
												nnnm = nclsnm + nnnm.substring(nnnm.indexOf("["));
											else
												nnnm = nclsnm;
											this.name = nnnm
										}
										else if(this.nodeName.toLowerCase()=="label")
										{
											var nnnm = this.getAttribute('orig_val_cls');
											if(nnnm.indexOf("[")!=-1)
												nnnm = nclsnm + nnnm.substring(nnnm.indexOf("["));
											else
												nnnm = nclsnm;
											this.name = nnnm
											this.innerHTML = nnnm
										}
									});
									key.lastclsnm = " " +nclsnm+" ";
								}
					        	#end
			
								function updatetawidcont()
								{
									var cont = JSON.stringify($('#$formName').serializeObject(), undefined, "\t");
									if(cont!="" && $('#req-txtarea').length>0)
									{
										$('#req-txtarea').val(cont).trigger('autosize.resize');
									}
								}
								
								var origHeight = 0;
								function adjustHeight()
								{
									var height = document.getElementById('$formName+Doc').offsetHeight 
													+ document.getElementById('$formName').parentNode.offsetHeight + 200;
									if(screen.height > height)
									{
										if(origHeight > screen.height)
											height = origHeight;
										else
											height = screen.height;
									}
									else if(origHeight > height)
									{
										height = origHeight;
									}
									document.getElementById('aside').style.height = height +'px';
								}
								
								function correctHeight()
								{
									document.getElementById('$formName+Doc').style.display = 'none';
									origHeight = document.getElementById('aside').offsetHeight;
									if(screen.height > origHeight)
									{
										origHeight = screen.height;
									}
									document.getElementById('$formName+Doc').style.display = 'block';
									var height = document.getElementById('$formName+Doc').offsetHeight 
											+ document.getElementById('$formName').parentNode.offsetHeight + 200;
									if(height > origHeight)
									{
										document.getElementById('aside').style.height = height +'px';
									}
									else 
										document.getElementById('aside').style.height = origHeight +'px';
								}
								
